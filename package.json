{
  "name": "generador-de-codigos",
  "version": "2.0.0",
  "main": "dist-node/index.js",
  "browser": "dist-browser/index.js",
  "types": "src/index.ts",
  "description": "Generador de codigos mediante opciones de inclusión [Longitud, Mayúsculas, Minúsculas, Números y Símbolos]",
  "author": "Leonardo Espejo (Leo-Spj)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leo-Spj/generador-de-contrasenas"
  },
  "homepage": "https://github.com/Leo-Spj/generador-de-contrasenas#readme",
  "license": "MIT",
  "scripts": {
    "build-browser": "tsc -p tsconfig.browser.json",
    "build-node": "tsc -p tsconfig.node.json",
    "build": "npm run build-browser && npm run build-node",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "keywords": [
    "generador",
    "códigos",
    "contraseña",
    "generator",
    "codes",
    "password"
  ]
}
